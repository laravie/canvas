{
    "name": "orchestra/canvas",
    "description": "Code Generators for Laravel Applications and Packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "bin": [
        "canvas"
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Canvas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Canvas\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2.0 <8.3.0",
        "composer-runtime-api": "^2.2",
        "composer/semver": "^3.0",
        "illuminate/database": "^11.0",
        "illuminate/support": "^11.0",
        "orchestra/canvas-core": "^9.0",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "laravel/framework": "^11.0",
        "orchestra/testbench": "^9.0",
        "phpstan/phpstan": "^1.10.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0-dev"
        },
        "laravel": {
            "providers": [
                "Orchestra\\Canvas\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php testbench package:discover --ansi",
        "ci": [
            "@composer audit",
            "@composer run prepare",
            "@php vendor/bin/phpstan analyse",
            "@php vendor/bin/pint"
        ],
        "test": "@php vendor/bin/phpunit -c ./ --color"
    },
    "minimum-stability": "dev"
}
